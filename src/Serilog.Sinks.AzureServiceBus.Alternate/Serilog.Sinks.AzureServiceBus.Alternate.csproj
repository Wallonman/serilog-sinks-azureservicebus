<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <RootNamespace>Serilog.Sinks.AzureServiceBus</RootNamespace>
    <ApplicationIcon />
    <StartupObject />
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Rahul Ruikar</Authors>
    <Company />
    <Description>Serilog.Sinks.AzureServiceBus is a serilog sink that writes logs to a queue in Azure Service Bus</Description>
    <RepositoryUrl>https://github.com/rahulruikar/serilog-sinks-azureservicebus</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Messaging.ServiceBus" Version="7.16.1" />
    <PackageReference Include="serilog" Version="3.0.1" />
  </ItemGroup>

</Project>
